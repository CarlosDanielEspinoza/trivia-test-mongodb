<%- include("./templates/header.ejs",{titleWeb : "Resultados"}) %>

<body>
    <header class="cabecera">
        <h1> Resultados </h1>
    </header>

    <section class="cuerpo">

        <article class="resultados">
            <table>
                <tr>
                    <th scope="col"> Name </th>
                    <th scope="col"> Age </th>
                    <th scope="col"> Points </th>
                </tr>
                <% if (data.length > 0) {%>
                    <% data.forEach( data => {%>

                        <tr>
                            <td> <%= data.name %> </td>
                            <td> <%= data.age %> </td>
                            <td> <%= data.point %> </td>
                        </tr>

                    <% }) %>
                <% } %>
            </table>
        </article>
    </section>

    <%- include("./templates/footer.ejs") %>

    <script src="/js/resultMain.js"></script>
</body>
</html>